{
  "expo": {
    "name": "Key Forecast",
    "slug": "key-forecast",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1117",
      "userInterfaceStyle": "automatic"
    },
    "updates": {
