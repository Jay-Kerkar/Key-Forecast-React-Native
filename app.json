    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "com.key.forecast",
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#0D1117"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "userInterfaceStyle": "automatic"
    },
    "description": ""
  }
}
